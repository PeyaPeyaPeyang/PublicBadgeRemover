{
    "manifest_version": 3,
    "name": "GitHub PublicBadge Remover",
    "version": "1.0.0",
    "author": "Peyang",
    "description": "Remove Public badges from github",
    "icons": {
        "16": "assets/icon/icon16.png",
        "64": "assets/icon/icon64.png",
        "128": "assets/icon/icon128.png"
    },
    "host_permissions": ["*://github.com/*/"],
    "content_scripts": [
        {
            "matches": ["*://github.com/*"],
            "js": ["src/main.js"],
            "run_at": "document_end"
        }
    ]
}
